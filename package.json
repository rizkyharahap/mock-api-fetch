  "engines": {
    "node": ">=20.17.0"
  },
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.esm.json && tsc -p tsconfig.cjs.json",
    "test": "jest",
    "test:watch": "jest --watch --coverage"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "esbuild": "^0.24.0",
    "esbuild-jest": "^0.5.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
